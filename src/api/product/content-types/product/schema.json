{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "new": {
      "type": "boolean"
    },
    "sale": {
      "type": "boolean"
    },
    "price": {
      "type": "decimal"
    },
    "originPrice": {
      "type": "decimal"
    },
    "quantity": {
      "type": "integer"
    },
    "description": {
      "type": "blocks"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "thumbImage": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "brand": {
      "type": "string"
    },
    "sold": {
      "type": "integer"
    },
    "quantityPurchase": {
      "type": "integer"
    },
    "action": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    }
  }
}
